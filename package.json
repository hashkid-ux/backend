{
  "name": "launch-ai-backend",
  "version": "2.0.0",
  "description": "Launch AI Platform - Production Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'No build step needed'",
    "postinstall": "npx playwright install chromium --with-deps || echo 'Playwright install failed, continuing...'",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "db:studio": "npx prisma studio",
    "railway:start": "npm run db:generate && npm run db:push && npm start"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": ["ai", "startup", "saas"],
  "author": "Launch AI",
  "license": "MIT",
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@prisma/client": "^6.18.0",
    "archiver": "^6.0.1",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "playwright": "^1.40.0",
    "prettier": "^3.1.0",
    "razorpay": "^2.9.2",
    "sentiment": "^5.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^6.18.0"
  }
}